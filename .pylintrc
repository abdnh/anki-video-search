[MASTER]
extension-pkg-whitelist=PyQt6,PyQt5
ignore=ankidata,venv,vendor,user_files

[REPORTS]
output-format=colorized

[MESSAGES CONTROL]
disable=
  missing-module-docstring,
  missing-class-docstring,
  missing-function-docstring,
  unused-argument,
  consider-using-f-string,

[BASIC]
good-names=mw
